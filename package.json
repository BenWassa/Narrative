{
  "name": "narrative",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,md,html}\" \"README.md\" \"docs/**/*.md\" \"docs/index.html\"",
    "lint": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,md,html}\" \"README.md\" \"docs/**/*.md\" \"docs/index.html\"",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage"
  },
  "vitest": {
    "environment": "jsdom",
    "setupFiles": "./vitest.setup.ts",
    "coverage": {
      "provider": "v8"
    }
  },
  "dependencies": {
    "lucide-react": "^0.259.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.0",
    "jsdom": "^27.3.0",
    "postcss": "^8.4.0",
    "prettier": "^2.8.0",
    "tailwindcss": "^3.4.8",
    "axe-core": "^4.12.1",
    "typescript": "^5.0.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  }
}
